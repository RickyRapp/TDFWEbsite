  "scripts": {
    "clean:build:production": "rimraf dist/production",
    "clean:build:dev": "rimraf dist/dev",
    "clean:build:staging": "rimraf dist/staging",
    "build:staging": "npm run clean:build:staging && cross-env NODE_ENV=staging webpack --config config/webpack.staging.js --bail",
    "build:production": "npm run clean:build:production && cross-env NODE_ENV=prod webpack --config config/webpack.prod.js --bail",
    "build:dev": "npm run clean:build:dev && cross-env NODE_ENV=dev webpack --config config/webpack.dev.js --bail",
    "start": "cross-env NODE_ENV=dev npm run server:dev",
    "start:hmr": "cross-env NODE_ENV=dev npm run server:dev:hmr",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --watch src/ --host 0.0.0.0",
    "server:dev:hmr": "npm run server:dev -- --inline --hot"
  },
  "devDependencies": {
    "@octopusdeploy/octopackjs": "0.1.0",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^9.4.4",
